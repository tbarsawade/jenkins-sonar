node {
  stage('SCM') {
    checkout scm
  }
  
  stage('SonarQube Analysis') {
    def msbuildHome = tool 'msbuild'
    def scannerHome = tool 'sonarscanner'
    withSonarQubeEnv() {
      bat "\"${scannerHome}\\SonarScanner.MSBuild.exe\" begin /k:\"mynndsaas\""
      bat "\"${msbuildHome}\" /property:Configuration=Release dev\\myndsaas.sln /t:Rebuild"
      bat "\"${scannerHome}\\SonarScanner.MSBuild.exe\" end"
    }
  }
}
